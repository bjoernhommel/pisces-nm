            "items": [
                { "itemId": "3...1", "itemStemText": "Ich versuche, einem Problem gemeinsam mit meinen Mitschüler/-innen auf den Grund zu gehen, um eine für alle gute Lösung zu finden", "responseFormatId": "RF1" },
                { "itemId": "4...2", "itemStemText": "Ich versuche im Allgemeinen, den Wünschen meiner Mitschüler/-innen gerecht zu werden", "responseFormatId": "RF1" },
                { "itemId": "2...3", "itemStemText": "Ich versuche, mich nach Möglichkeit nicht festzulegen", "responseFormatId": "RF1" },
                { "itemId": "1...4", "itemStemText": "Ich behalte meine Meinung gerne für mich", "responseFormatId": "RF1" },
                { "itemId": "3...5", "itemStemText": "Ich versuche, meine Ideen mit denen meiner Mitschüler/-innen abzustimmen, um gemeinsam zu einer Entscheidung zu gelangen", "responseFormatId": "RF1" },
                { "itemId": "3...6", "itemStemText": "Ich bemühe mich, mit meinen Mitschüler/-innen die Lösung für ein Problem zu finden, die alle Erwartungen erfüllt ", "responseFormatId": "RF1" },
                { "itemId": "3...7", "itemStemText": "Ich vermeide gewöhnlich offene Diskussionen über verschiedene Meinungen mit meinen Mitschüler/-innen ", "responseFormatId": "RF1" },
                { "itemId": "2...8", "itemStemText": "Ich setze mich durch, um meine Ideen durchzusetzen", "responseFormatId": "RF1" },
                { "itemId": "3...9", "itemStemText": "Ich nutze meine Stärke, um meinen Willen durchzusetzen ", "responseFormatId": "RF1" },
                { "itemId": "2...10", "itemStemText": "Ich komme den Wünschen meiner Mitschüler/-innen normalerweise entgegen ", "responseFormatId": "RF1" },
                { "itemId": "3...11", "itemStemText": "Ich gebe den Wünschen meiner Mitschüler/-innen nach ", "responseFormatId": "RF1" },
                { "itemId": "3...12", "itemStemText": "Ich investiere viel Zeit, um mit meinen Mitschüler/-innen lange Informationen auszutauschen, um ein Problem gemeinsam zu lösen ", "responseFormatId": "RF1" },
                { "itemId": "3...13", "itemStemText": "Ich mache meinen Mitschüler/-innen gegenüber im Allgemeinen Zugeständnisse ", "responseFormatId": "RF1" },
                { "itemId": "4...14", "itemStemText": "Ich versuche, Meinungsverschiedenheiten mit meinen Mitschüler/-innen aus dem Weg zu gehen", "responseFormatId": "RF1" },
                { "itemId": "4...15", "itemStemText": "Ich vermeide Auseinandersetzungen mit meinen Mitschüler/-innen", "responseFormatId": "RF1" },
                { "itemId": "4...16", "itemStemText": "Ich nutze mein besseres Wissen, um andere auszutricksen", "responseFormatId": "RF1" },
                { "itemId": "4...17", "itemStemText": "Ich schließe mich den Vorschlägen meiner Mitschüler/-innen oft an ", "responseFormatId": "RF1" },
                { "itemId": "4...18", "itemStemText": "Ich verfahre nach dem Prinzip „geben und nehmen“, so dass ein Kompromiss gefunden werden kann ", "responseFormatId": "RF1" },
                { "itemId": "4...19", "itemStemText": "Ich verfolge meine Interessen im Allgemeinen ohne auf andere zu hören ", "responseFormatId": "RF1" },
                { "itemId": "2...20", "itemStemText": "Ich versuche, all unsere Wünsche offenzulegen, so dass Probleme gut gelöst werden können ", "responseFormatId": "RF1" },
                { "itemId": "4...21", "itemStemText": "Ich arbeite mit meinen Mitschüler/-innen zusammen, um für alle gute Entscheidungen zu bekommen ", "responseFormatId": "RF1" },
                { "itemId": "1...22", "itemStemText": "Ich versuche, die Erwartungen meiner Mitschüler/-innen zu erfüllen ", "responseFormatId": "RF1" },
                { "itemId": "3...23", "itemStemText": "Ich mache manchmal etwas heimlich, um mich gegen andere durchzusetzen ", "responseFormatId": "RF1" },
                { "itemId": "4...24", "itemStemText": "Ich versuche, meine abweichende Meinung gegenüber Mitschüler/-innen für mich zu behalten, um kein böses Blut zu schaffen ", "responseFormatId": "RF1" },
                { "itemId": "4...25", "itemStemText": "Ich versuche, Streit über Meinungen mit meinen Mitschüler/-innen zu vermeiden ", "responseFormatId": "RF1" },
                { "itemId": "4...26", "itemStemText": "Ich versuche, mit meinen Mitschüler/-innen zu einem gemeinsamen Verständnis des Problems zu gelangen ", "responseFormatId": "RF1" }
            ]
        }
